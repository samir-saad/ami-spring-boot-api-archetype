<archetype-descriptor
        xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0
        https://maven.apache.org/xsd/archetype-descriptor-1.1.0.xsd"
        xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        name="spring-boot-api-archetype">
    <fileSets>
        <fileSet encoding="UTF-8" filtered="true" packaged="true">
            <directory>src/main/java</directory>
        </fileSet>
        <fileSet encoding="UTF-8" filtered="true" packaged="false">
            <directory>src/main/resources</directory>
        </fileSet>
        <fileSet encoding="UTF-8" filtered="true" packaged="true">
            <directory>src/test/java</directory>
        </fileSet>
        <fileSet encoding="UTF-8" filtered="true" packaged="false">
            <directory></directory>
            <includes>
                <include>.gitignore</include>
                <include>Jenkinsfile</include>
                <include>pipeline-config.json</include>
                <include>README.md</include>
            </includes>
        </fileSet>
    </fileSets>
</archetype-descriptor>